{
  "name": "miband-watchface-tool",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run build:backend && npm run build:frontend",
    "build:frontend": "parcel build --dist-dir dist/assets src/index.html",
    "build:backend": "cd server && tsc && cp -rf dist/ ../dist",
    "docker:build": "docker build -t pixel-app .",
    "docker:start": "docker run --name wf-app -d -p 80:80 pixel-app",
    "docker:stop": "docker stop wf-app || true && docker rm -f wf-app || true",
    "start": "node dist/app",
    "dev": "node scripts/dev",
    "parse": "tsc && node app/parse",
    "pack": "tsc && node app/pack",
    "render": "tsc && node app/render"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@parcel/packager-raw-url": "^2.2.1",
    "@parcel/transformer-sass": "^2.2.1",
    "@parcel/transformer-webmanifest": "^2.2.1",
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.15",
    "@types/sharp": "^0.29.5",
    "nodemon": "^2.0.15",
    "parcel": "^2.2.1",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "express": "^4.17.2",
    "express-async-handler": "^1.2.0",
    "sharp": "^0.30.0"
  }
}
